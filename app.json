{
  "expo": {
    "name": "SG3Icon",
    "slug": "Download-Img",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/combined_icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splashscreen.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "splash": {
        "image": "./assets/splashscreen.png",
        "resizeMode": "cover",
        "backgroundColor": "#00000000"
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/combined_icon.png",
        "backgroundColor": "#00000000"
      }
    },
    "web": {
      "favicon": "./assets/logoWeb48x48byGPT.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ]
  }
}
